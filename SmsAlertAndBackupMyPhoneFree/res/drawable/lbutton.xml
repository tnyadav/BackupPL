<?xml version="1.0" encoding="utf-8"?>
<selector
xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true" >
<shape>
<gradient
android:startColor="#F3E2A9"
android:endColor="#BCF5A9"
android:angle="270" />
<stroke
android:width="1dp"
android:color="#5A0505" />
<corners
android:radius="2dp" />
<padding
android:left="10dp"
android:top="10dp"
android:right="10dp"
android:bottom="10dp" />
</shape>
</item>
<item android:state_focused="true" >
<shape>
<gradient
android:startColor="#F3E2A9"
android:endColor="#BCF5A9"
android:angle="270" />
<stroke
android:width="1dp"
android:color="#5A0505" />
<corners
android:radius="2dp" />
<padding
android:left="10dp"
android:top="10dp"
android:right="10dp"
android:bottom="10dp" />
</shape>
</item>
<item>
<shape>
<gradient
android:startColor="#A9F5F2"
android:endColor="#A9A9F5"
android:angle="270" />
<stroke
android:width="1dp"
android:color="#5A0505" />
<corners
android:radius="2dp" />
<padding
android:left="10dp"
android:top="10dp"
android:right="10dp"
android:bottom="10dp" />
</shape>
</item>
</selector>